# Environment Configuration for ASR Project

# OpenMP Library Conflict Fix (macOS)
KMP_DUPLICATE_LIB_OK=TRUE

# Application Settings
ENVIRONMENT=development
HOST=0.0.0.0
PORT=8087
LOG_LEVEL=debug

# Whisper Model Settings (using correct field names)
WHISPER_MODEL_SIZE=large-v3
WHISPER_DEVICE=auto

# TTS Settings
TTS_MODEL_NAME=tts_models/multilingual/multi-dataset/xtts_v2

# Performance Settings
WORKERS=1
ENABLE_METRICS=true

# Security Settings - Comment out to use defaults from config.py
# ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000,http://localhost:8087
# ALLOWED_HOSTS=localhost,127.0.0.1 